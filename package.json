{
    "name": "evaluation-api",
    "version": "0.0.0",
    "description": "Backend Developer Task",
    "private": true,
    "scripts": {
        "build": "yarn db && tsc",
        "db": "yarn prisma generate",
        "dev": "nodemon src/main.ts",
        "start": "node dist/index.js",
        "test:server:up": "docker-compose -f docker-compose.test.yml up --build",
        "test:e2e": "jest --config test/jest.config.ts --testPathPattern=test/index.e2e.test.ts",
        "lint": "eslint 'src/**/*.{js,ts}'",
        "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
        "format": "prettier --write 'src/**/*.{js,ts,json,md}'",
        "prepare": "husky",
        "prisma:migrate": "prisma migrate deploy",
        "prisma:migrate:dev": "prisma migrate dev",
        "prisma:migrate:prod": "prisma migrate deploy",
        "prisma:generate": "prisma generate",
        "prisma:db:push": "prisma db push",
        "prisma:introspect": "prisma introspect",
        "prisma:studio": "prisma studio",
        "db:populate": "ts-node src/scripts/docker-entrypoint.sh"
    },
    "dependencies": {
        "@prisma/client": "^5.15.0",
        "@types/bcrypt": "^5.0.2",
        "axios": "^1.7.2",
        "bcrypt": "^5.1.1",
        "bullmq": "^5.7.15",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "jsonwebtoken": "^9.0.2",
        "reflect-metadata": "^0.2.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@jest/globals": "^29.7.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^20.14.2",
        "@typescript-eslint/eslint-plugin": "^7.12.0",
        "@typescript-eslint/parser": "^7.12.0",
        "docker-compose": "^0.24.8",
        "eslint": "^9.4.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "globals": "^15.3.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.5",
        "nodemon": "^3.1.3",
        "prettier": "^3.3.1",
        "prisma": "^5.15.0",
        "ts-jest": "^29.1.4",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.12.0"
    },
    "main": "index.js",
    "repository": "git@github.com:relaxedrohan/evaluation-api.git",
    "author": "Rohan Yadav <rohanyadavdec@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": "^v20.14.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint",
            "prettier --write"
        ]
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ]
    }
}
